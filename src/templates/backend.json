{
	"filename": "backend",
	"html": "./src/templates/backend.html",
	"position": 2,
	"addExpress": {
		"addExpress": {
			"npm": ["express cors helmet express-validator"],
			"run": { "zone": "backend", "command": ["mkdir ./routes"] },
			"file": {
				"zone": "backend",
				"fileSource": "./src/templates/express/index.js",
				"fileDest": "./index.js"
			},
			"file1": {
				"zone": "backend",
				"fileSource": "./src/templates/express/userRoutes.js",
				"fileDest": "./routes/userRoutes.js"
			}
		},
		"type": "checkbox"
	},
	"addDb": {
		"type": "radio",
		"addMongoose": {
			"npm": ["mongoose"],
			"run": { "zone": "backend", "command": ["mkdir ./db"] },
			"file": {
				"zone": "backend",
				"fileSource": "./src/templates/mongoose/database.js",
				"fileDest": "./db/database.js"
			}
		},
		"addPostgreSql": {
			"run": { "zone": "backend", "command": ["echo PostregSQL N/A"] }
		}
	}
