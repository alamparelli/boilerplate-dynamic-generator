{
	"filename": "frontend",
	"html": "./src/templates/frontend.html",
	"position": 0,
	"selectFrontend": {
		"type": "radio",
		"reactjs": {
			"run": ["npx create-react-app ."],
			"npm": [
				"eslint-plugin-react-hooks eslint-plugin-react @eslint/compat react react-router-dom"
			]
		},
		"reactts": {
			"run": ["npx create-react-app . --template typescript"],
			"npm": [
				"eslint-plugin-react-hooks eslint-plugin-react @eslint/compat react react-router-dom"
			]
		},
		"angular": {
			"run": ["echo 'nothing to do for Angular'"]
		},
		"noFrontend": {
			"run": ["npm init -y"]
		}
	}
}
